Dline1:                         D1   # chose D1 or D2 line to simulate 
Dline2:                         D1   # chose D1 or D2 line to simulate 
excited_hpf_state1:             F2   # specify the excited hpf F
excited_hpf_state2:             F2   # specify the excited hpf F
polarization1:                 pi   # polarization for transition from ground F = 1
polarization2:                 pi    # polarization for transition from ground F = 2  
I1:                            5    # [mW/cm^2]  laser intensity corresponding to polarization1
I2:                            0   # [mW/cm^2]  laser intensity corresponding to polarization2
detune1:    		       0   # [Hz]  laser detune corresponding to polarization1
detune2:                       0   # [Hz]  laser detune corresponding to polarization1
dt:                          10e-9    # [s]  simulation time step size 
maxSimulationTime:           500e-6    # [s]  max simulation time, auto stop if rearch steady state